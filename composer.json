{
	"name": "baraja-core/variable-generator",
	"description": "Generate new variable symbol by last variable and selected strategy.",
	"homepage": "https://github.com/baraja-core/variable-generator",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.4.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"tracy/tracy": "^2.7",
		"phpstan/phpstan-nette": "^0.12.6",
		"symplify/easy-coding-standard": "^7.2",
		"spaze/phpstan-disallowed-calls": "^1.1",
		"baraja-core/doctrine": "^2.4",
		"nette/di": "^3.0"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
